use 5.010001;
use strict;
use warnings;

use Perinci::CmdLine::Any;

Perinci::CmdLine::Any->new(
    url => '/Git/Bunch/',
    subcommands => {
        ls     => { url => '/Git/Bunch/list_bunch_contents' },
        check  => { url => '/Git/Bunch/check_bunch'  },
        sync   => { url => '/Git/Bunch/sync_bunch'   },
        exec   => { url => '/Git/Bunch/exec_bunch'   },
    },
    log => 1,
)->run;

1;
